<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.seecs.muzahir.e_quizzer.instructor">

    <EditText
        android:id="@+id/qstitle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="28dp"
        android:layout_marginEnd="34dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="171dp"
        android:ems="10"
        android:hint="Write Title Here"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/startq"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:onClick="oncreateb"
        android:id="@+id/startq"
        android:layout_width="169dp"
        android:layout_height="48dp"
        android:layout_marginBottom="39dp"
        android:text="Create Quiz"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/qstitle" />

    <Button
        android:id="@+id/button3"
        android:onClick="logoutf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="137dp"
        android:layout_marginEnd="40dp"
        android:text="Log out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/startq"
        app:layout_constraintTop_toBottomOf="@+id/startq" />
</android.support.constraint.ConstraintLayout>
